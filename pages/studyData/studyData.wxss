.point{
  position: absolute;
  width: 1rpx;
  height: 1rpx;
  background: #000;
}
.point .i{
  width: 30rpx;
  height: 30rpx;
  border-radius: 50%;
  background: #f09345;
  position: absolute;
  top: 0;
  left: 0;
  transform: translate(-50%, -50%);

}
.point .text{
  position: absolute;
  width: 300rpx;
  text-align: center;
  transform:translate(-50%, 40%);
}
.point .smalltext {
transform:translate(-20%, -100%);
}

