/* pages/chapterLists/chapterLists.wxss */
.navTop{
  display: flex;
  display: -webkit-flex;
  background: #fbc93d;
}
.ntchild{
  width: 50%;
  text-align: center;
}
.ntctxt{
  padding: 15rpx 20rpx;
  box-sizing: border-box;
  font-size: 36rpx;
  color: #fff;
  display: inline-block;
}
.ntctxtSelected{
  border-bottom: 6rpx solid #ff9900;

}
.h1nav{
  padding: 25rpx 5%;
  box-sizing: border-box;
  position: relative;
  font-size: 30rpx;
  color: #ff9900;
  font-weight: bold;
  border-bottom: 1px solid #e9e9e9;
}
.h1icon{
  display: inline-block;
  width: 29rpx;
  height: 23rpx;
  background: url("data:image/png;base64,") no-repeat;
background-size: 100%;
position: absolute;
right: 5%;
top: 50%;
transform: translate(0,-50%)
}
.hierarchy2{
  padding: 0 5%;
  box-sizing: border-box;
}
.h2nav{
  padding: 25rpx 160rpx 25rpx 0;
  border-bottom: 1px solid #e9e9e9;
  box-sizing: border-box;
  position: relative;
}
.h2T1{
  font-size: 28rpx;
  color: #333;
  padding-left: 40rpx;
  position: relative;
}
.h2T2{
  font-size: 22rpx;
  color: #999;
  padding-left: 40rpx;
  padding-top: 10rpx;
}
.i1{
   background: url("data:image/png;base64,") no-repeat;
}
.i2{
  background: url("data:image/png;base64,") no-repeat;
}
.h2icon{
  display: inline-block;
  width: 27rpx;
  height: 27rpx;
 
  background-size: 100%;
  position: absolute;
  top: 8rpx;
  left: 0;
}
.h2T2 text{
  margin-right: 25rpx;
}
.h3nav{
  padding: 25rpx 140rpx 25rpx 0;
  border-bottom: 1px solid #e9e9e9;
  box-sizing: border-box;
  position: relative;
}
.h3icon{
  display: inline-block;
  width: 14rpx;
  height: 14rpx;
  background: #fbc93d;
  position: absolute;
  top: 15rpx;
  left: 10rpx;
  border-radius: 50%;
}
.pointcount{
  width: 140rpx;
  height: 100%;
  font-size: 22rpx;
  color: #fbc93d;
  padding-right: 38rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 17rpx 28rpx;
  background-position: 100% 50%;
  position: absolute;
  right: 0;
  top: 0;
  display: inline-flex;
  display: -webkit-inline-flex;
  align-items: center;
  justify-content: flex-end;
  box-sizing: border-box;
}
.share{
  display: inline-block;
  background: #ff9900;
  font-size: 26rpx;
  color: #fff;
  padding: 15rpx 25rpx;
  border-radius: 100rpx;
  position: absolute;
  right: 0;
  top: 50%;
  transform: translate(0,-50%)
}
.summarybox{
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
}
.shadowbox{
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  background: rgba(0, 0, 0, 0.5);
  padding-right: 60%;
  display: flex;
  display: -webkit-flex;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
  font-size: 26rpx;
  color: #fff;
}
.summarymain{
  background: #fff;
  right: 0;
  position: fixed;
  top: 0;
  width: 60%;
  overflow-y: scroll;
  -webkit-overflow-scrolling: touch;
  height: 100%;
  bottom: 0;
}
.summaryHeader{
  font-size: 30rpx;
  color: #000;
  font-weight: bold;
  padding: 60rpx;
  border-bottom: 1px solid #e9e9e9;
}
.pointchangetext{
  color: #ff9900;
  position: absolute;
  right: 0;
  top: 0;
}
.pointchange{
  font-size: 22rpx;
  color: #999;
  padding-right: 90rpx;
  position: relative;
  margin-top: 15rpx;
}
.summarylist{
  font-size: 28rpx;
  color: #333;
  padding: 35rpx 0 35rpx 50rpx;
  border-bottom: 1px solid #e9e9e9;
  position: relative;
}
.closemenu{
  width: 42%;
  line-height: 45rpx;
}
.sharebtn{
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  z-index: 9;
}
.selectSelectbox{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  display: flex;
  display: -webkit-flex;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
}
.ssShadow{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.5);
}
.ssmain{
  width: 92%;
  max-height: 60%;
  background: #fff;
  border: 1px solid #fbc93d;
  overflow-y: scroll;
  -webkit-overflow-scrolling: touch;
  z-index: 2;
}
.ssList{
  font-size: 26rpx;
  color: #333;
  padding: 30rpx 25rpx;
  box-sizing: border-box;
  border-bottom: 1px solid #fbc93d;
}
.ssList:last-child{
  border-bottom: 0;
}
.ssSlected{
  background: #ffeec0;
}
.chapterselected{
  background: #ffefc4;
}
.has_learnen::before{
  content: '';
  display:inline-block;
  width:24rpx;
  height:19rpx;
  background:url("data:image/png;base64,") no-repeat;
  background-size: 100% 100%;
  position:absolute;
  top:50%;
  left:20rpx;
  margin-top: -7rpx;
}
.gou{
  display:inline-block;
  width:21rpx;
  height:17rpx;
  background:url("data:image/png;base64,") no-repeat;
  background-size: 100% 100%;
}