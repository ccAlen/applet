/* pages/typeSelection/typeSelection.wxss */
.tsTitle{
  padding: 60rpx 5%;
  box-sizing: border-box;
  text-align: center;
}
.tstchild{
  font-size: 46rpx;
  font-weight: bold;
  color: #93512f;
  display: inline-block;
  position: relative;  
}
.tstchild::before{
  content: '';
  display: inline-block;
  width: 100%;
  height: 30rpx;
  background: #fff2c9;
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: -2;
}
.typecontent{
  padding: 100rpx 15% 60rpx;
  box-sizing: border-box;
}
.typechild{
  padding: 25rpx 15rpx;
  box-sizing: border-box;
  border: 1px solid #93512f;
  font-size: 32rpx;
  color: #93512f;
  margin-bottom: 22rpx;
  border-radius: 25rpx;
  text-align: center;
}
.selected{
  background: #fbc93d;
  border: 1px solid #fbc93d;
}
.nextstepbox{
  display: box;
  display: -webkit-box;
  /* padding: 0 15%; */
  box-sizing: border-box;
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
}
.previousStep,.nextStep{
  box-flex: 1;
  -webkit-box-flex: 1;
  font-size: 26rpx;
  color: #93512f;
  height: 120rpx;
  line-height: 120rpx;
  text-align: center;
}
.nextchild{
  display: inline-block;
  /* text-align: right; */
  padding-right: 30rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 12rpx 21rpx;
  background-position: 100% 50%;
}
.prechild{
  display: inline-block;
  padding-left: 30rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 12rpx 21rpx;
  background-position: 0 50%;
}