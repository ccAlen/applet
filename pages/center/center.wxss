/* pages/center/center.wxss */
page{
  background: #f9fafc;
}
.cban{
  display: block;
  width: 750rpx;
  height: 377rpx;
}
.centerBanner{
  position: relative;
}
.cenbanCon{
  position: absolute;
  top: 0;
  left: 0;
  padding: 60rpx 6%;
  box-sizing: border-box;
  display: flex;
  display: -webkit-flex;
  width: 100%;
}
.cbcLeft{
  width: 160rpx;
  display: flex;
  display: -webkit-flex;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
}
.headerimg{
  display: inline-block;
  width: 140rpx;
  height: 140rpx;
  border-radius: 50%;
  overflow: hidden;
  border: 6rpx solid #fff;
}
.cbcRight{
  color: #fff;
  font-size: 26rpx;
  padding-left: 40rpx;
  width:60%;
}
.useName{
  font-size: 36rpx;
}
.changeInfo{
  padding: 15rpx 0 5rpx;
}
.centermain{
  display: flex;
  flex-wrap:wrap;
  /* justify-content: center; */
  align-content: flex-start;
  padding: 80rpx 5%;
  box-sizing: border-box;
}
.centerchild{
  width: 25%;
  text-align: center;
  margin-bottom: 50rpx;
  position: relative;
}
.centericon{
  display: inline-block;
  width: 82rpx;
  height: 82rpx;
}
.cf{
  font-size: 26rpx;
  color: #666;
  padding-top: 16rpx;
  text-align: center;
}
.groupcontent{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 9;
  display: flex;
  display: -webkit-flex;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
}
.shadowbox{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.6)
}
.groupmain{
  background: #fff;
  border-radius: 25rpx;
  padding: 84rpx 20rpx;
  position: relative;
  width: 70%;
  text-align: center;
}
.codeimg{
  display: inline-block;
  width: 250rpx;
  height: 250rpx;
}
.gT1{
  font-size: 26rpx;
  color: #333;
}
.closeicon{
  display: inline-block;
  width: 100rpx;
  height: 100rpx;
  position: absolute;
  right: 0;
  top: 0;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 25rpx 25rpx;
  background-position: 50% 50%;
}
.contactbtn{
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  z-index: 9;
}