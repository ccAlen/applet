/* pages/list/list.wxss */
page{
  background: #f9fafc;
}
.h1nav{
  padding: 25rpx 5%;
  box-sizing: border-box;
  position: relative;
  font-size: 30rpx;
  color: #ff9900;
  font-weight: bold;
  background: #fff;
}
.hierarchy2{
  padding: 0 2% 40rpx;
  box-sizing: border-box;
}
.h2nav{
  padding: 25rpx 90rpx 25rpx 0;
  box-sizing: border-box;
  position: relative;
  background: url('data:image/png;base64,') no-repeat #fff;
  background-size: 47rpx 47rpx;
  background-position: 95% 50%;
  border-radius: 20rpx;
  margin-top: 25rpx;
  box-shadow: 0 5px 16px 2px #eee;
}
.h2T1{
  font-size: 28rpx;
  color: #333;
  padding-left: 40rpx;
  position: relative;
}
.h2T2{
  font-size: 22rpx;
  color: #999;
  padding-left: 40rpx;
  padding-top: 10rpx;
}
.i1{
  background: url("data:image/png;base64,") no-repeat;
}
.i2{
  background: url("data:image/png;base64,") no-repeat;
}
.iconbox{
  display: inline-block;
  padding-left: 26rpx;
  background-size: 23rpx 23rpx;
  background-position: 0 50%;
  margin-right: 40rpx;
}
.startingwork{
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  display: flex;
  display: -webkit-flex;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
}
.shadowbox{
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background: rgba(0, 0, 0, 0.5)
}
.workmain{
  background: #fff;
  border-radius: 25rpx;
  padding: 60rpx 4% 50rpx;
  box-sizing: border-box;
  text-align: center;
  z-index: 2;
  width: 80%;
}
.wT2{
  font-size: 24rpx;
  color: #999;
  line-height: 45rpx;
}
.wT1{
  font-size: 30rpx;
  color: #333;
  text-align: center;
  padding: 0 0 30rpx;
}
.startbtn{
  display: inline-block;
  width: 90%;
  height: 80rpx;
  line-height: 80rpx;
  color: #fff;
  font-size: 28rpx;
  border-radius: 100rpx;
  text-align: center;
  background: #fbc93d;
  margin-top: 60rpx;
}