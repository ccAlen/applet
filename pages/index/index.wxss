/* pages/blog1/canvas.wxss */
 page{
   background: #f9fafc;
 }
.container {
  position: relative;
}
.banimg{
  display: block;
  width: 750rpx;
  height: 500rpx;
}
.bantextbox{
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
}
.progress_box {
  position: relative;
  width: 100px;
  height: 100px;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: transparent;
  margin-left: 50rpx;
}
 
.progress_bg {
  position: absolute;
  width: 100px;
  height: 100px;
}
 
.progress_canvas {
  width: 100px;
  height: 100px;
}
 
.progress_text {
  position: absolute;
  display: flex;
  align-items: center;
  justify-content: center;
  /* width:160rpx;
  background:#fff;
  height:160rpx;
  border-radius:50%; */

}
 
.progress_info {
  font-size: 1em;
  padding-left: 16rpx;
  letter-spacing: 2rpx;
  color: #fff;
  font-weight: bold;
}
.studybox{
  display: flex;
  display: -webkit-flex;
  margin-bottom: 45rpx;
}
.studyLeft{
  width: 60%;
  padding: 50rpx 0 0 0;
  box-sizing: border-box;
  text-align: center;
}
.selectbox{
  text-align: center;
  padding: 10rpx 0 20rpx;
}
.select{
  overflow: hidden;white-space: nowrap;text-overflow: ellipsis;
  display: inline-block;
  width: 60%;
  text-align: left;
  font-size: 28rpx;
  padding: 16rpx 45rpx 16rpx 30rpx;
  border-radius: 20rpx;
  color: #333;
  box-sizing: border-box;
  background: url("data:image/png;base64,") #fff no-repeat;
  background-size: 17rpx 10rpx;
  background-position: 95% 50%;
}
.studytxt{
  position: absolute;
  bottom: -32rpx;
  left: 0;
  font-size: 26rpx;
  color: #333;
  text-align: center;
  width: 100%;
}
.slChild1{
  color: #fff;
  display: flex;
  display: -webkit-flex;
  justify-content: center;
}
.day{
  font-size: 36rpx;
  padding: 0 20rpx;
  border-right: 1px solid #fff;
  line-height: 36rpx;
  font-weight: bold;
}
.time{
  font-size: 36rpx;
  padding: 0 20rpx;
  line-height: 36rpx;
  font-weight: bold;
}
.slChild2{
  text-align: center;
  font-size: 26rpx;
  color: #fff;
  padding: 22rpx 0;
}
.btnbox{
  text-align: center;
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
}
.studybtn{
  display: inline-block;
  width: 50%;
  background: #fff;
  border-radius: 100rpx;
  text-align: center;
  font-size: 30rpx;
  color: #ff9900;
  padding: 20rpx 0;
}
.studybtntxt{
  font-size: 20rpx;
  color: #fff;
  text-align: center;
  padding: 15rpx 0;
}
.indexmain{
  padding: 25rpx 20rpx;
  box-sizing: border-box;
}
.indexchild{
  background: #fff;
  border-radius: 25rpx;
  box-shadow: 0 4px 15px 0 #ddd;
  display: flex;
  padding: 25rpx 20rpx;
  box-sizing: border-box;
  margin-bottom: 20rpx;
  border: 1rpx solid #fbd56b;
}
.indexchild:last-child{
  margin: 0;
}
.ic_left{
  width: 150rpx;
  text-align: center;
}
.icon{
  display: inline-block;
  width: 123rpx;
  height: 113rpx;
}
.font1{
  font-size: 30rpx;
  color: #333;
}
.ic_bom{
  display: flex;
  padding-top: 10rpx;
}
.font2{
  font-size: 22rpx;
  color: #999;
}
.ic_right{
  display: flex;
  display: -webkit-flex;
  align-items: center;
  box-sizing: border-box;
  padding-left: 40rpx;
  width: 100%;
}
.percent,.answer{
  width: 50%;
  padding-left: 30rpx;
}
.vwidth{
  width: 100%;
}
.percent{
  background: url("data:image/png;base64,") no-repeat;
  background-size: 23rpx 23rpx;
  background-position: 0 50%;
}
.answer{
  background: url("data:image/png;base64,") no-repeat;
  background-size: 23rpx 23rpx;
  background-position: 0 50%;
}
.boombox{
  position: fixed;
  top: 90rpx;
  left: 0;
  bottom: 0;
  right: 0;
  display: flex;
  display: -webkit-flex;
  justify-content: center;
  padding-top: 60rpx;
}
.shadowbox{
  position: fixed;
  top: 90rpx;
  left: 0;
  bottom: 0;
  right: 0;
  background: rgba(0, 0, 0, 0.5);
}
.boomMain{
  background: #fff;
  border-radius: 25rpx;
  width: 92%;
  max-height: 75%;
  z-index: 2;
  overflow-y: scroll;
  -webkit-overflow-scrolling: touch;
}
.boomNav{
  display: box;
  display: -webkit-box;
  border-bottom: 1px solid #e9e9e9;
}
.boomNavChild{
  box-flex: 1;
  -webkit-box-flex: 1;
  text-align: center;
  padding: 30rpx 20rpx 30rpx 0;
  box-sizing: border-box;
  font-size: 32rpx;
  color: #999;
  background: url("data:image/png;base64,") no-repeat;
  background-size: 15rpx 18rpx;
  background-position: 100% 50%;
}
.boomNavChild:last-child{
  background: none;
}
.navselected{
  color: #fbc93d;
  position: relative;
}
.navselected::after{
  display: inline-block;
  content: '';
  width: 60%;
  height: 6rpx;
  background: #fbc93d;
  position: absolute;
  bottom: 0;
  left: 20%;
  margin-left: -10rpx;
}
.boomListbox{
  padding: 60rpx 8%;
  box-sizing: border-box;
}
.boomList{
  font-size: 32rpx;
  color: #93512f;
  text-align: center;
  border: 1px solid #93512f;
  border-radius: 25rpx;
  height: 90rpx;
  line-height: 90rpx;
  margin-bottom: 20rpx;
}
.selected{
  background: #fbc93d;
  border: 1px solid #fbc93d;
}